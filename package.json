{
  "name": "frontend",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --tunnel",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prettify": "prettier --write .",
    "test": "jest ./Tests",
    "test-debug": "jest ./Tests --watch --debug"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|firebase|@firebase)"
    ],
    "setupFiles": [
      "./jestConfig.js",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**src/**/*.{js,jsx}",
      "!**/debugScreens/**",
      "!**/styles/**",
      "!**/mockData.js"
    ]
  },
  "dependencies": {
    "@expo/cli": "^0.10.11",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-navigation/native": "^6.1.3",
    "@react-navigation/native-stack": "^6.9.9",
    "@types/react": "^18.2.15",
    "@types/react-native": "^0.72.2",
    "add": "^2.0.6",
    "axios": "^1.3.4",
    "eslint-plugin-jest": "^27.2.1",
    "expo": "^49.0.4",
    "expo-cli": "^6.3.2",
    "expo-status-bar": "^1.6.0",
    "firebase": "^10.1.0",
    "jest": "^29.6.1",
    "jest-expo": "^49.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.72.3",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "^3.22.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.19.7",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@expo/ngrok": "^2.5.0",
    "@testing-library/react-native": "^12.0.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "jest-mock-axios": "^4.7.1",
    "prettier": "^3.0.0"
  },
  "private": true
}
